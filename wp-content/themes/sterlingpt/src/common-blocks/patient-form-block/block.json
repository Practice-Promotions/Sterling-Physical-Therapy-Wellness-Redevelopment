{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 2,
    "name": "ppcoreblocks/patient-form-block",
    "version": "1.0.0",
    "title": "Patient Form",
    "category": "common_block",
    "icon": "pdf",
    "description": "Add a form PDF image, title, and download button for easy access to patient forms.",
    "supports": {
        "html": false
    },
    "attributes": {
        "content": {
            "type": "string",
            "default": ""
        },
        "color": {
            "type": "string",
            "default": "black"
        },
        "customClass": {
            "type": "string",
            "default": ""
        }
    },
    "textdomain": "patient-form-block",
    "editorScript": "file:./index.js"
}
