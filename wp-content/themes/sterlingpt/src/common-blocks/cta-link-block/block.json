{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 2,
    "name": "ppcoreblocks/cta-link-block",
    "version": "1.0.0",
    "title": "CTA Link",
    "category": "common_block",
    "icon": "admin-links",
    "description": "Display Anchor link Block.",
    "supports": {
        "html": false
    },
    "attributes": {
        "linkType": {
            "type": "string",
            "default": "page"
        },
        "pageLink": {
            "type": "string",
            "default": ""
        },
        "externalLink": {
            "type": "string",
            "default": ""
        }
    },
    "textdomain": "cta-link-block",
    "editorScript": "file:./index.js"
}
