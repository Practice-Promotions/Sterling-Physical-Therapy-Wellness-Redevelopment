{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"version": "0.0.1",
	"name": "testimonial-block/testimonial-meta-injector",
	"title": "Query Loop (Testimonials Meta)",
	"icon": "id",
	"parent": ["testimonial-block"],
	"category": "widgets",
	"description": "Displays custom meta information within core query loop for testimonial.",
	"attributes": {
		"testimonial_title": {
			"type": "string",
			"default": ""
		},
		"enable_testimonial_title": {
			"type": "boolean",
			"default": true
		}
    },
	"usesContext": ["postId", "postType"],
	"supports": {
		"align": true
	},
	"example": {},
	"textdomain": "testimonial-meta-injector",
	"editorScript": "file:./index.js",
	"render": "file:./render.php"
}
