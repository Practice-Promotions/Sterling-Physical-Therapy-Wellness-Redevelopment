{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 3,
    "version": "0.0.1",
    "name": "ppcoreblocks/video-block",
    "title": "Video Block",
    "icon": "video-alt3",
    "category": "common",
    "description": "Add a popup video block for YouTube, Vimeo, and uploaded videos.",
    "attributes": {
        "videoType": {
            "type": "string",
            "default": ""
        },
        "videoUrl": {
            "type": "string",
            "default": ""
        },
        "videoId": {
            "type": "string",
            "default": ""
        },
        "coverImage": {
            "type": "string",
            "default": ""
        }
    },
    "supports": {
        "align": true
    },
    "textdomain": "herostencilpt",
    "editorScript": "file:./index.js",
    "editorStyle": "file:./index.css",
    "style": "file:./style-index.css"
}
